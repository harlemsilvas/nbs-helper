# Porta do servidor (padrão: 3001)
PORT=3001

# Ambiente (development, production)
NODE_ENV=development

# URL do dataset (JSON com todos os códigos NBS)
DATASET_URL=https://nbs-helper-web.vercel.app/index.json

# Rate limiting - Máximo de requisições por janela de tempo
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=900000

# Cache - Tempo de vida do cache em segundos
CACHE_TTL=3600

# CORS - Origens permitidas (separadas por vírgula, * para permitir todas)
CORS_ORIGIN=*
