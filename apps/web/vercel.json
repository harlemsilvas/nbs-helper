{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "redirects": [
    {
      "source": "/",
      "destination": "https://nbs-helper.vercel.app/app",
      "permanent": true
    },
    {
      "source": "/:path((?!app).*)",
      "destination": "https://nbs-helper.vercel.app/app/:path",
      "permanent": true
    }
  ]
}
